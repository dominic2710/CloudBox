<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CloudBox.Photo.Pages.ViewPhotoPage"
             xmlns:views="clr-namespace:CloudBox.Photo.Views"
             xmlns:viewModels="clr-namespace:CloudBox.Photo.ViewModels"
             Shell.TabBarIsVisible="False"
             Title="ViewPhotoPage">
    <ContentPage.BindingContext>
        <viewModels:ViewPhotoViewModel/>
    </ContentPage.BindingContext>
    <Grid>
        <views:PhotoItemView Url="{Binding Url}"/>
    </Grid>
</ContentPage>